{#
/**
 * @file
 * Default theme implementation for webform submission.
 *
 * Available variables:
 * - elements: An array of elements to display in view mode.
 * - webform_submission: The webform submission.
 * - webform: The webform.
 *
 * @see template_preprocess_webform_submission()
 *
 * @ingroup themeable
 */
#}
{%
  set classes = [
  'webform-submission',
  'webform-submission--webform-' ~ webform.id()|clean_class,
  webform_submission.isSticky() ? 'webform-submission--sticky',
  view_mode ? 'webform-submission--view-mode-' ~ view_mode|clean_class,
]
%}
<div class="container">
  {{ statusTag }}
</div>
<div class="webform-submission__title-wrapper">
  {{ drupal_block('page_title_block') }}
</div>
<div class="container">
  <div class="webform-submission__application_id">
    <h2 class="webform-submission__application_id--header">{{ "Application number"|t }}</h2>
    <div class="webform-submission__application_id--body">{{ applicationNumber }}</div>
  </div>
<div{{ attributes.addClass(classes) }}>

  {% if statusString != 'DRAFT' %}
  <div id="message-form-wrapper">
    {{ messages }}
  </div>
  {% endif %}


  {{ information }}

  {{ submission }}
</div>
</div>
